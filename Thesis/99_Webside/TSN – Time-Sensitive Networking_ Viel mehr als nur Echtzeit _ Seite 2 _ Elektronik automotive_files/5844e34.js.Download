(function (){
    $(window).scroll(function () {
        var windowTop = $(window).scrollTop();
        var headerTop = $('#sticky-anchor').offset().top;

        if (windowTop > headerTop) {
            $('header.navigation').addClass('navbar-fixed-top');
            $('header.navigation > .row').addClass('container center');
            $('#sticky-anchor').height($('header.navigation').outerHeight());
        } else {
            $('header.navigation').removeClass('navbar-fixed-top');
            $('header.navigation > .row').removeClass('container center');
            $('#sticky-anchor').height(0);
        }
    });
})();

(function (){
    $("#mobile_search").on("click", function (){
        var query = $('#search_form');
        var isVisible = query.is(':visible');

        if (isVisible === true) {
            $("#search_form").hide();
        } else {
            $("#search_form").show();
            $("#mobile_search").removeClass("btn btn-link visible-xs visible-sm mobile-search-ico");
            $("#mobile_search").hide();
        }
    });

    $("#searchbox-close").on("click", function () {
        $("#search_form").hide();
        $("#mobile_search").css("display", "block");
        $("#mobile_search").addClass("btn btn-link visible-xs visible-sm");
    });
})();

(function (){
    $( document ).ready( function() {
        /**
         * Show full text
         *
         * @param event
         */
        function showFullText(event) {
            event.preventDefault();

            var companyId = this.getAttribute('data-company-id');
            var fullText = document.getElementById('fullTextCompany-'+companyId),
                shortText = document.getElementById('shortTextCompany-'+companyId);

            fullTextClasses = fullText.classList;
            if (fullTextClasses.contains('hide')) {
                fullTextClasses.remove('hide');
            }

            shorTextClasses = shortText.classList;
            if (!shorTextClasses.contains('hide')) {
                shorTextClasses.add('hide');
            }
        }

        /**
         * Show less text
         *
         * @param event
         */
        function showLessText(event) {
            event.preventDefault();

            var companyId = this.getAttribute('data-company-id');
            var fullText = document.getElementById('fullTextCompany-'+companyId),
                shortText = document.getElementById('shortTextCompany-'+companyId);

            shorTextClasses = shortText.classList;
            if (shorTextClasses.contains('hide')) {
                shorTextClasses.remove('hide');
            }

            fullTextClasses = fullText.classList;
            if (!fullTextClasses.contains('hide')) {
                fullTextClasses.add('hide');
            }
        }

        // Links to show and hide full text
        var moreLink = document.getElementsByClassName('more-link'),
            lessLink = document.getElementsByClassName('less-link');

        // Iterate if links are found
        if (moreLink.length) {
            for (i = 0; i < moreLink.length; i++) {
                moreLink[i].addEventListener("click", showFullText, false);
            }
        }

        if (lessLink.length) {
            for (i = 0; i < lessLink.length; i++) {
                lessLink[i].addEventListener("click", showLessText, false);
            }
        }
    });
})();

